function main() {
    def y = 1-2j;
    def a = 5 + j;
    def b = koty(y,a);
    def c = 5;
    sum(c);
    print(b);
    c = 5;
    b = 4;
    y = silnia(c) * silnia(b);
    print(y);
    y = 15;
