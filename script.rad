function sum(a) {
    def z = 0;
    if(a != z) {
        return;
    }
    a = a - 1;
    print(a);
    sum(a);
}

function silnia(n) {
    if(n <= 1) {
        return n;
    } else {
        def x = n-1;
        return n * silnia(x);
    }
}

function fib(n) {
    if(n <= 1) {
        return n;
    }
    else {
        def x = n - 1;
        def y = n - 2;
        def z = fib(x) + fib(y);
        return z;
    }
}

