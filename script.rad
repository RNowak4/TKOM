}

function suma() {
    def a = 0;
    def b = 2;
    def c = 2;
    def d = 2;
    return a - (b + b) * d;
}

function pow(a,n) {
    if(n == 0) {
        return 1;
    } else {
        n = n - 1;
        return a * pow(a,n);
    }
}

